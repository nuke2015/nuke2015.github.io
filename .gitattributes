# Set the default behavior, in case people don't have core.autocrlf set.
* text eol=lf
*.png   -text
*.jpg   -text
*.jpeg   -text
*.gif   -text
*.bin   -text
*.dll   -text
*.exe   -text
*.wps   -text
*.doc   -text
*.docx   -text
*.xls   -text
*.xlsx   -text
*.ppt   -text
*.pptx   -text
*.zip   -text
*.rar   -text
*.tar.gz   -text
*.7z   -text
*.pdf   -text
*.ttf   -text
*.eot   -text
*.woff   -text
*.woff2   -text
*.mp3   -text
*.mp4   -text
*.webm   -text
*.flv   -text
*.swf   -text
*.ico   -text
*.woff   -text
*.otf   -text
*.z   -text
*.bz   -text
*.tar   -text
