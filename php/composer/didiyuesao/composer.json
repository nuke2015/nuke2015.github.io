{
    "require": {
        "topthink/think": "5.0.9",
        "pingplusplus/pingpp-php": "dev-master",
        "overtrue/wechat": "~3.1",
        "cboden/ratchet": "^0.3.6",
        "krok/phpquery": "0.9.5.3",
        "Facebook/WebDriver": "^1.4",
        "nuke2015/api": "^2017.06",
        "didiyuesao/api": "*@dev"
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        },
        {
            "packagist": false
        },
        {
            "type": "path",
            "url": "packages/*/*"
        }
    ],
    "autoload": {
        "psr-4": {
            "didiyuesao\\api\\base": "vender/nuke2015/api/base"
        }
    }
}